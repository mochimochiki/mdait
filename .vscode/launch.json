{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "${workspaceFolder}/sample",
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--profile-temp"
      ],
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "${defaultBuildTask}",
      "skipFiles": ["**/app/out/vs/**"]
    }
  ]
}
